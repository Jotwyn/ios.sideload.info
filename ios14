    <script>
    // --- Page Slide Transition Animation ---
    function slideToPage(href) {
        document.body.classList.remove('slide-in-left');
        document.body.classList.add('slide-out-left');
        setTimeout(function() {
            window.location.href = href;
        }, 500);
    }
    // Nav links
    document.querySelectorAll('nav a').forEach(function(link) {
        link.addEventListener('click', function(e) {
            if (link.href && !link.href.endsWith(window.location.pathname)) {
                e.preventDefault();
                slideToPage(link.getAttribute('href'));
            }
        });
    });
    // Slide-in on load
    window.addEventListener('pageshow', function() {
        document.body.classList.remove('slide-out-left');
        document.body.classList.add('slide-in-left');
    });
    </script>
